HUGGINGFACEHUB_API_TOKEN=your_token_here

# ===== MODEL =====
API_KEY=
API_BASE_URL=https://generativelanguage.googleapis.com/v1beta/openai/
MODEL_NAME=gemini-2.5-flash-lite
MAX_TOKENS=4096
TEMPERATURE=0.0
PROXY=
# ===== SECONDARY (Draft) MODEL =====
DRAFT_API_KEY=
DRAFT_API_BASE_URL=https://generativelanguage.googleapis.com/v1beta/openai/
DRAFT_MODEL_NAME=gemini-2.5-flash-lite
MAX_TOKENS_DRAFT=4096
TEMPERATURE_DRAFT=0.0
DRAFT_PROXY=
# ===== VERIFIER MODEL =====
VERIFIER_API_KEY=
VERIFIER_API_BASE_URL=https://generativelanguage.googleapis.com/v1beta/openai/
VERIFIER_MODEL_NAME=gemini-2.5-pro
VERIFIER_MAX_TOKENS=4096
VERIFIER_TEMPERATURE=0.0
VERIFIER_PROXY=
# ===== DRAFT/VERIFIER SETTINGS =====
# Enable draft/verifier pattern for parallel tool calls
USE_DRAFT_VERIFIER=true
# Number of parallel draft candidates to generate
DRAFT_CANDIDATES=3
# Whether to use verifier model to validate candidates
USE_VERIFIER=true

